<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Soft golden/tan special prize gradient -->
    <gradient
        android:type="radial"
        android:gradientRadius="50%"
        android:centerX="0.3"
        android:centerY="0.3"
        android:startColor="#FFF8E1"
        android:centerColor="#FFE082"
        android:endColor="#FFCA28" />

    <stroke
        android:width="4dp"
        android:color="#8D6E63" />

    <size
        android:width="60dp"
        android:height="60dp" />

</shape>


